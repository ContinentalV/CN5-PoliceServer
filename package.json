{
  "name": "api-rest",
  "version": "1.0.0",
  "description": "API REST FOR DASH/BOT POLICE",
  "main": "index.js",
  "scripts": {
    "start:dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "init:full-app": "node init/init-db.ts && sequelize db:migrate && npx sequelize-cli db:seed:all",
    "init:db:create": "node init/init-db.ts",
    "init:db:migrate": "sequelize db:migrate",
    "init:db:seedall": "npx sequelize-cli db:seed:all",
    "stop": "taskkill /im node.exe /f",
    "start:prod": "node build/server.js",
    "dev": "nodemon src/app.ts",
    "build": "tsc"
  },
  "author": "neo-getsu",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/connect-timeout": "^0.0.39",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.6",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "connect-timeout": "^1.9.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsdoc": "^4.0.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.5",
    "querystring": "^0.2.1",
    "reflect-metadata": "^0.2.1",
    "remove": "^0.1.5",
    "sequelize": "^6.37.1",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.19",
    "winston": "^3.11.0"
  }
}
